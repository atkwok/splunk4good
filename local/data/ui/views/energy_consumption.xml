<dashboard>
  <label>Energy Consumption</label>
  <row>
    <panel>
      <title>Hourly Energy Consumption - Seattle City Light</title>
      <chart>
        <title></title>
        <search>
          <query>(index="s4gproj" source="seattleenergyusage.json") | sort _time | table _time megawatthours</query>
          <earliest>0</earliest>
          <latest>1498892400</latest>
        </search>
        <option name="charting.chart">line</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <title>Hourly Temperature - Seattle</title>
      <chart>
        <search>
          <query>index="s4gproj" sourcetype=weatherdata | where (tempi&lt;150 AND tempi&gt;-150) AND pressurei&gt;0 | sort _time | table _time tempi</query>
          <earliest>0</earliest>
          <latest>1498892400</latest>
        </search>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.chart">line</option>
        <option name="charting.chart.nullValueMode">connect</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.layout.splitSeries.allowIndependentYRanges">1</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
</dashboard>